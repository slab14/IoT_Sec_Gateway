{
    "description": "v0 - 1 device, 1 snort container, ping demo",
    "nf_platform": "docker",
    "in_ip": "192.1.1.2",
    "out_ip": "10.1.1.2",
    "n_devices": 1,
    "policy": {
	"device_0": {
	    "state_machine": "binary",
	    "DAG": "binary_chain"
	}
    }, 
    "FSM_defs": {
	"binary": {
	    "0": "ping_detect",
	    "1": "ping_block",
	    "n": "2"
	}
    },
    "DAG_defs": {
	"binary_chain":  {
	    "ping_detect": "jhelt/click-bridge snort_icmp_alert",
	    "ping_block": "jhelt/click-bridge snort_icmp_block"
	}
    } 
}
